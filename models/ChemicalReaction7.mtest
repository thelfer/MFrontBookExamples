@Author Thomas Helfer;
@Date   09/08/2022;
@Model 'src/libBehaviour.so' 'ChemicalReaction7';

@Real 'B0' 0.1;
@StateVariable 'MolarConcentrationOfSpeciesA' '0';
@StateVariable 'MolarConcentrationOfSpeciesB' 'B0';

@ExternalStateVariable 'Temperature' 293.15;

@Times {
  0, 360 in 100
};

// unit testing
@Real 'k01' 'ChemicalReaction7::ReferenceReactionRateCoefficientAB';
@Real 'k02' 'ChemicalReaction7::ReferenceReactionRateCoefficientBA';
@Real 'Ta1' 'ChemicalReaction7::ActivationTemperatureAB';
@Real 'Ta2' 'ChemicalReaction7::ActivationTemperatureBA';
@Real 'k1' 'k01 * exp(-Temperature / Ta1)';
@Real 'k2' 'k02 * exp(-Temperature / Ta2)';
@Real 'K'  'k1 + k2';
@Real 'B'  'k2 * B0';
@Test<function> 'MolarConcentrationOfSpeciesA' '(B/K) * (1 - exp(-K * t))'      1e-5;
@Test<function> 'MolarConcentrationOfSpeciesB' 'B0 - (B/K) * (1 - exp(-K * t))' 1e-5;